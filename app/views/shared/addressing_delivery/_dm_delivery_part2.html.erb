<div class='col-md-5' id='ncoa'>
  <%= f.input :ncoa, as: :boolean, label: 'NCOA' %>
</div>
<div class='col-md-4'>
  <%= f.input :cass_certified, as: :boolean, label: 'CASS Certified' %>
</div>
<div class='clear_with_height'></div>
<div class='col-md-4' id='delivery_guarantee_pct'>
  <%= f.input :delivery_guarantee_pct, label: 'Delivery Guarantee Percent', input_html: {class: 'form-control col-xs-3'} %>
</div>
<div class='clear_with_height'></div>
<div class='col-md-12' id='delivery_guarantee_desc'>
  <%= f.input :delivery_guarantee_desc, as: :text, label: 'Delivery Guarantee Other',
              input_html: {class: 'form-control col-xs-10 text_area_resizing', rows:3} %>
</div>